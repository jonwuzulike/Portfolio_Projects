<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE  hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.ers.models.User" table="users">
<id name="id" type="int" column="user_id"></id>
<property name="first" type="string" column="first_name"></property>
<property name="last" type="string" column="last_name"></property>
<property name="email" type="string" column="email"></property> 
<property name="userTypeId" type="int" column="user_type_id"></property>
</class>

<class name="com.ers.models.Reimbursement" table="reimbursement">
<id name="rId" type="int" column="r_id"></id>
<property name="name" type="string" column="name"></property>
<property name="amount" type="double" column="amount"></property>
<property name="rStatusId" type="int" column="r_status_id"></property>
<property name="rTypeId" type="int" column="r_type_id"></property>
<property name="userId" type="int" column="user_id"></property>
<property name="date" type="string" column="time_submitted"></property>
</class>
</hibernate-mapping>